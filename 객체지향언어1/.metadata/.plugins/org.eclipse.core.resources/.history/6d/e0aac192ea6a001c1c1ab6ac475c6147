package N3;

public class Car {

	String company = "현대";
	String model;
	String color;
	int maxSpeed;
	
	Car(String model, String color, int maxSpeed) {
		this.model = model;
		this.color = "은색";
		this.maxSpeed = 250;
	}
	
	void print() {
		System.out.println(model + " " + color + " " + maxSpeed);
	}
}
