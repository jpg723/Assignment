package N3;

public class Car {

	String company = "현대";
	String model;
	String color = "은색";
	int maxSpeed = 250;
	
	Car(String model, String color, int maxSpeed) {
		this.model = model;
		this.color = color;
		this.maxSpeed = maxSpeed;
	}
	
	void print() {
		System.out.println(model + " " + color + " " + maxSpeed);
	}
}
