package N3;

public class Car {

	String company = "현대";
	String model;
	String color;
	int maxSpeed;
	
	Car(String model) {
		this.model = model;
		this.color = "은색";
		this.maxSpeed = 250;
	}
	Car(String model, String color) {
		this.model = model;
		this.color = color;
		this.maxSpeed = 250;
	}
	Car(String model, String color, int maxSpeed) {
		this.model = model;
		this.color = color;
		this.maxSpeed = maxSpeed;
	}
}
