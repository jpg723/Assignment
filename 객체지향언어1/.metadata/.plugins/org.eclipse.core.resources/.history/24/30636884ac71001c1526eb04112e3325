package P12_5;

public class Undergraduate {
	int year;
	
	public Undergraduate(int year) {
		super(id, tuition, gpa);
		this.year = year;
	}
	
	public void setYear(int year) {
		this.year = year;
	}
	
	public int getYear() {
		return year;
	}
	
	public String toString() {
		return "id= " + super.id + " tuition= " + tuition + " gpa = " + gpa + " 장학금= " + 0;
	}

}
