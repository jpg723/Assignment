
public class P {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		A a = new A("홍길동", 3000, 0);
		
		System.out.println(a.getName());
	}

}

class A{
	
	private String name;
	private int salary;
	int age;
	
	public A(String n, int s, int a) {
		name = n;
		salary = s;
		age = a;
	}
	
	public String getName() {
		return name;
	}
	
	public int getSalary() {
		return salary;
	}
	
	public int getAge() {
		return age;
	}
}
