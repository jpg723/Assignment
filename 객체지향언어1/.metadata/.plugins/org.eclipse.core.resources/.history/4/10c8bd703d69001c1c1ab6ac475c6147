package A7_1;

public class Date {
	
	private int year;
	private int month;
	private int day;
	private int age;
	
	public Date(int y, int m, int d) {
		year = y;
		month = m;
		day = d;		
	}
	
	public void setAge() {
		
		age = 2021 - year;
	}
	
	public int getAge() {
		
		return age;
	}

	void print() {
		
		System.out.println("당신의 생일은 " + year + "년 " + month + "월 " + day + "일입니다.");
	}

}
