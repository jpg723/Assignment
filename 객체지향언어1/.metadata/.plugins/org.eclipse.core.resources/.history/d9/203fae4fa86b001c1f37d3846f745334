
public class P {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		A a = new A(3);
		
		System.out.println(a.calcArea());
	}

}

class A{
	int radius;
	
	public A(int radius) {
		this.radius = radius;
	}
	
	public A() {
		this(0);
	}
	
	double calcArea() {
		return 3.14 * radius * radius;
	}
}
