public class A5_6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		@SuppressWarnings("resource")
		Scanner sc = new Scanner(System.in);
		
		Address a = new Address();
		
		System.out.print("이름을 입력하세요: ");
		a.name = sc.next();
